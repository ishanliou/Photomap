<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>






<% @users.each do |u| 
    userPic = []%>
    <% @pic.each do |pic| %>
                <% if pic.user_id == u.id %>
                    <% userPic.push(pic.url)%>
                <% end %>
            <% end %>
<div class="card" style="width: 18rem; height: 18rem;">

  <img class="card-img-top" src="<%= userPic.first %>" alt="Card image cap">
  <div class="card-body">
    <h5 class="card-title"><%= link_to(u.name, user_path(u))%></h5>
    <p class="card-text"><%= %></p>

  </div>
</div>
<% end %> 