<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<h1>Discover</h1>
<ul>   
   

        <% 
            @users.each do |u|
            userPic = []
        %>
            <% @pic.each do |pic| %>
                <% 
                    if pic.user_id == u.id
                        userPic.push(pic.url)
                %>
                <% end %>
            <% end %>
            <li> 
                <%= link_to(u.name, user_path(u))%> 
                <img src='<%= userPic[0] %>'>
            </li>
        <% end %> 

        <!--
        <% @pic.each do |test| %>
            <%= link_to image_tag(test.url), user_picture_path(@user, test) %>
            <%= debug test %>
        <% end %>
        -->
</ul>
